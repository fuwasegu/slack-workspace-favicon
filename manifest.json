{
    "name": "SlackWorkspaceFavicon",
    "version": "1.2",
    "manifest_version": 3,
    "permissions": ["activeTab"],
    "icons": {
        "16": "imgs/icon16.png",
        "48": "imgs/icon48.png",
        "128": "imgs/icon128.png"
    },
    "content_scripts": [
        {
            "matches": ["https://app.slack.com/client/*"],
            "js": ["script.js"]
        }
    ]
}
